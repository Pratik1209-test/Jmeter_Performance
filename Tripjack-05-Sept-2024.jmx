              <stringProp name="parameters"></stringProp>
              <stringProp name="script">// Obtain the response data from the previous request
String responseText = prev.getResponseDataAsString()

// Define the regular expression pattern to extract the OTP value
def pattern = ~/otpId=(\d+)/

// Create a matcher to find the OTP value
def matcher = pattern.matcher(responseText)

// Check if the pattern is found in the response
if (matcher.find()) {
    // Extract the OTP value from the group
    def otpValue = matcher.group(1)
    
    // Log the extracted value (useful for debugging)
    log.info(&quot;Extracted OTP Value: &quot; + otpValue)
    
    // Save the extracted OTP value into a JMeter variable
    vars.put(&quot;extractedOtpValue&quot;, otpValue)
} else {
    log.error(&quot;OTP value not found in the response.&quot;)
}
</stringProp>
